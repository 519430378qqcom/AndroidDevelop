<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:background">@color/app_backgroud</item>
    </style>
    <style name="AppTheme.base" parent="AppTheme">
        <item name="android:textColor">@color/black_3</item>
    </style>
    <style name="Apptheme_develop" parent="@style/AppTheme.base">
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <!--文本style-->
    <style name="text_black3_14sp">
        <item name="android:textColor">@color/black_3</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="text_black9_14sp">
        <item name="android:textColor">@color/black_9</item>
        <item name="android:textSize">14sp</item>
    </style>
</resources>
